{
  "default": "cloudflare",
  "cdns": {
    "cloudflare": {
      "name": "Cloudflare CDN (via GitHub)",
      "baseUrl": "https://cdn.jsdelivr.net/gh/JayEmVey/gate7@master/dist/",
      "description": "Fast Cloudflare edge with jsDelivr routing"
    },
    "jsdelivr": {
      "name": "jsDelivr CDN (Convert from Github)",
      "baseUrl": "https://cdn.jsdelivr.net/gh/JayEmVey/gate7@latest",
      "description": "Global CDN routing optimization"
    },
    "github": {
      "name": "GitHub Raw (Fallback)",
      "baseUrl": "https://raw.githubusercontent.com/JayEmVey/gate7/master",
      "description": "GitHub raw content fallback"
    }
  },
  "assetPaths": {
    "css": "/css",
    "js": "/js",
    "images": "/images"
  },
  "fallbackStrategy": "github",
  "timeout": 8000,
  "retryAttempts": 2,
  "buildDate": "2025-11-23",
  "version": "1.0.0",
  "enableDetailedLogging": true,
  "activeCdn": {
    "name": "Cloudflare CDN",
    "provider": "cloudflare",
    "baseUrl": "https://cdn.jsdelivr.net/gh/JayEmVey/gate7@master/dist/"
  }
}